<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.seguros.presupuestos"
    android:versionCode="16"
    android:versionName="4.08" >

    <uses-sdk
        android:minSdkVersion="19"
        android:targetSdkVersion="28" />

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.READ_PHONE_STATE" />
    <uses-permission android:name="android.permission.GET_ACCOUNTS" />
    <uses-permission android:name="android.permission.ACCESS_WIFI_STATE" />
    <uses-permission android:name="android.permission.CHANGE_WIFI_STATE" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.READ_CONTACTS" />
    <uses-permission android:name="android.permission.WAKE_LOCK" />
    <uses-permission android:name="com.google.android.finsky.permission.BIND_GET_INSTALL_REFERRER_SERVICE" /> <!-- Required by older versions of Google Play services to create IID tokens -->
    <uses-permission android:name="com.google.android.c2dm.permission.RECEIVE" />

    <permission
        android:name="com.seguros.presupuestos.permission.C2D_MESSAGE"
        android:protectionLevel="signature" />

    <uses-permission android:name="com.seguros.presupuestos.permission.C2D_MESSAGE" />

    <application
        android:allowBackup="true"
        android:appComponentFactory="android.support.v4.app.CoreComponentFactory"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:requestLegacyExternalStorage="true"
        android:theme="@style/AppTheme"
        android:usesCleartextTraffic="true" >
        <activity android:name="com.seguros.presupuestos.Activity_Premios" />
        <activity android:name="com.seguros.presupuestos.Prelogin" /> <!-- Samsung multi-window support -->
        <uses-library
            android:name="com.sec.android.app.multiwindow"
            android:required="false" />
        <!--
 Set custom default icon. This is used when no icon is set for incoming notification messages.
             See README(https://goo.gl/l4GJaQ) for more.
        -->
        <meta-data
            android:name="com.google.firebase.messaging.default_notification_icon"
            android:resource="@drawable/ic_compania" />
        <!--
 Set color used with incoming notification messages. This is used when no color is set for the incoming
             notification message. See README(https://goo.gl/6BKBk7) for more.
        -->
        <meta-data
            android:name="com.google.firebase.messaging.default_notification_color"
            android:resource="@color/tresprov" />
        <meta-data
            android:name="com.google.firebase.messaging.default_notification_channel_id"
            android:value="@string/canal_notify" />
        <meta-data
            android:name="com.sec.android.support.multiwindow"
            android:value="true" />

        <activity
            android:name="com.seguros.presupuestos.SplashActivity"
            android:theme="@style/SplashTheme" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.seguros.presupuestos.Menu"
            android:label="@string/app_name" />
        <activity
            android:name="com.seguros.presupuestos.Activacion"
            android:label="Activar Aplicacion" />
        <activity
            android:name="com.seguros.presupuestos.Activacion2"
            android:label="Activar Aplicacion" />
        <activity
            android:name="com.seguros.presupuestos.LoginApp"
            android:label="Login User" >
            <intent-filter>
                <category android:name="android.intent.category.MULTIWINDOW_LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.seguros.presupuestos.Sepelio"
            android:label="Presupuesto Seguro Sepelio" />
        <activity
            android:name="com.seguros.Actualizacion.MenuActual"
            android:label="ActualizaciÃ³n de Precios" />
        <activity
            android:name="com.seguros.presupuestos.ListServicios"
            android:label="Presupuesto Seguro Vida" />
        <activity
            android:name="com.seguros.presupuestos.NuevoSepelio"
            android:label="Nuevo Integrante Seguro Sepelio"
            android:theme="@android:style/Theme.Holo.Light.Dialog" />
        <activity
            android:name="com.seguros.presupuestos.NuevoVida"
            android:label="Integrantes Seguro de Vida"
            android:theme="@android:style/Theme.Holo.Light.Dialog" />
        <activity
            android:name="com.seguros.pdf.GenerarPDFActivity"
            android:label="Compartir Presupuesto"
            android:theme="@android:style/Theme.Holo.Light.Dialog" />
        <activity
            android:name="com.seguros.pdf.GenerarPDFActivitySep"
            android:label="Compartir Presupuesto"
            android:theme="@android:style/Theme.Holo.Light.Dialog" />
        <activity
            android:name="com.seguros.presupuestos.MenuContactos"
            android:label="Contactenos...." />

        <service
            android:name="com.seguros.presupuestos.Publicaciones"
            android:enabled="true"
            android:process=":ServiceVerifyState" />

        <receiver android:name="com.seguros.presupuestos.VerificarWifi" >
            <intent-filter>
                <action android:name="android.net.conn.CONNECTIVITY_CHANGE" />
            </intent-filter>
        </receiver>

        <activity
            android:name="com.seguros.presupuestos.ActivityAyuda"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name="com.seguros.presupuestos.ActivityVendedor"
            android:theme="@style/AppTheme"
            android:windowSoftInputMode="stateHidden" />
        <activity android:name="com.seguros.presupuestos.ListaVendedores" />
        <activity android:name="com.seguros.MainActivity" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.MULTIWINDOW_LAUNCHER" />
                />
            </intent-filter>
        </activity>
        <activity android:name="com.seguros.presupuestos.Adicionales" />
        <activity
            android:name="com.seguros.presupuestos.SepelioCalc"
            android:theme="@android:style/Theme.Holo.Light.Dialog" />
        <activity
            android:name="com.seguros.Cocherias.BuscaxDni"
            android:theme="@style/Base.Theme.AppCompat.Light.Dialog" />
        <activity
            android:name="com.seguros.Cocherias.FamiliaActivity"
            android:theme="@style/Base.Theme.AppCompat.Light.Dialog" />
        <activity android:name="com.seguros.Cocherias.DatosAsegurados" />
        <activity android:name="com.seguros.presupuestos.MenuAsegurado" />
        <activity
            android:name="com.seguros.Cocherias.BuscarAsegurado"
            android:label="@string/app_name"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity android:name="com.seguros.presupuestos.Calendario" />
        <activity android:name="com.seguros.presupuestos.OtrosServicios" />
        <activity android:name="com.seguros.presupuestos.Login_Asegurado" />
        <activity android:name="com.seguros.presupuestos.Registro_Asegurado" />
        <activity android:name="com.seguros.presupuestos.Modifica_Asegurado" />
        <activity android:name="com.seguros.presupuestos.Modifica_CBU" />
        <activity android:name="com.seguros.presupuestos.InfoPushActivity" />
        <activity android:name="com.seguros.presupuestos.Clave_adm" />
        <activity android:name="com.seguros.presupuestos.Blanqueo_Asegurado" />
        <activity
            android:name="com.seguros.presupuestos.MostrarInfo"
            android:theme="@android:style/Theme.Holo.Light.Dialog" />

        <service
            android:name="com.seguros.fcm.CustomFirebaseMessagingService"
            android:exported="false" >
            <intent-filter>
                <action android:name="com.google.firebase.MESSAGING_EVENT" />
            </intent-filter>
        </service>
        <service android:name="com.seguros.fcm.CustomFirebaseInstanceIDService" >
            <intent-filter>
                <action android:name="com.google.firebase.INSTANCE_ID_EVENT" />
            </intent-filter>
        </service>
        <!--
             FirebaseMessagingService performs security checks at runtime,
             no need for explicit permissions despite exported="true"
        -->
        <service
            android:name="com.google.firebase.messaging.FirebaseMessagingService"
            android:exported="true" >
            <intent-filter android:priority="-500" >
                <action android:name="com.google.firebase.MESSAGING_EVENT" />
            </intent-filter>
        </service>
        <service android:name="com.google.firebase.components.ComponentDiscoveryService" >
            <meta-data
                android:name="com.google.firebase.components:com.google.firebase.analytics.connector.internal.AnalyticsConnectorRegistrar"
                android:value="com.google.firebase.components.ComponentRegistrar" />
            <meta-data
                android:name="com.google.firebase.components:com.google.firebase.iid.Registrar"
                android:value="com.google.firebase.components.ComponentRegistrar" />
        </service>

        <receiver
            android:name="com.google.android.gms.measurement.AppMeasurementReceiver"
            android:enabled="true"
            android:exported="false" >
        </receiver>
        <receiver
            android:name="com.google.android.gms.measurement.AppMeasurementInstallReferrerReceiver"
            android:enabled="true"
            android:exported="true"
            android:permission="android.permission.INSTALL_PACKAGES" >
            <intent-filter>
                <action android:name="com.android.vending.INSTALL_REFERRER" />
            </intent-filter>
        </receiver>

        <service
            android:name="com.google.android.gms.measurement.AppMeasurementService"
            android:enabled="true"
            android:exported="false" />
        <service
            android:name="com.google.android.gms.measurement.AppMeasurementJobService"
            android:enabled="true"
            android:exported="false"
            android:permission="android.permission.BIND_JOB_SERVICE" />

        <receiver
            android:name="com.google.firebase.iid.FirebaseInstanceIdReceiver"
            android:exported="true"
            android:permission="com.google.android.c2dm.permission.SEND" >
            <intent-filter>
                <action android:name="com.google.android.c2dm.intent.RECEIVE" />

                <category android:name="com.seguros.presupuestos" />
            </intent-filter>
        </receiver>
        <!--
             FirebaseInstanceIdService performs security checks at runtime,
             no need for explicit permissions despite exported="true"
        -->
        <service
            android:name="com.google.firebase.iid.FirebaseInstanceIdService"
            android:exported="true" >
            <intent-filter android:priority="-500" >
                <action android:name="com.google.firebase.INSTANCE_ID_EVENT" />
            </intent-filter>
        </service>

        <provider
            android:name="com.google.firebase.provider.FirebaseInitProvider"
            android:authorities="com.seguros.presupuestos.firebaseinitprovider"
            android:exported="false"
            android:initOrder="100" />

        <activity
            android:name="com.google.android.gms.common.api.GoogleApiActivity"
            android:exported="false"
            android:theme="@android:style/Theme.Translucent.NoTitleBar" />

        <meta-data
            android:name="com.google.android.gms.version"
            android:value="@integer/google_play_services_version" />
    </application>

</manifest>